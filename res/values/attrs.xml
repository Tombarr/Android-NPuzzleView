<?xml version="1.0" encoding="utf-8"?>
<resources>
     <declare-styleable name="CoverFlow">
        <attr name="imageWidth" format="dimension" />
        <attr name="imageHeight" format="dimension" />
        <attr name="withReflection" format="boolean" />
        <attr name="reflectionGap" format="dimension" />
        <attr name="imageReflectionRatio" format="float" />
    </declare-styleable>
    <declare-styleable name="DockPanel">
		<attr name="handleButtonDrawableId" format="reference" />
		<attr name="contentLayoutId" format="reference" />
		<attr name="dockPosition" format="enum" />
		<attr name="isOpen" format="boolean" />
		<attr name="animationDuration" format="integer" />
	</declare-styleable>
	<declare-styleable name="NPuzzleView">
		<attr name="difficulty" format="integer" />
		<attr name="countdownSeconds" format="integer" />
		<attr name="border" format="reference" />
		<attr name="src" format="reference" />
	</declare-styleable>
</resources>